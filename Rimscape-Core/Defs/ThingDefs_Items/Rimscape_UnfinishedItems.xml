<?xml version="1.0" encoding="utf-8" ?>
<Defs>


  <ThingDef Name="RS_UnfinishedBase" Abstract="True">
    <thingClass>UnfinishedThing</thingClass>
    <category>Item</category>
    <label>unfinished thing</label>
    <graphicData>
      <texPath>Things/Item/Unfinished/UnfinishedSculpture</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <altitudeLayer>Item</altitudeLayer>
    <useHitPoints>true</useHitPoints>
    <isUnfinishedThing>true</isUnfinishedThing>
    <scatterableOnMapGen>false</scatterableOnMapGen>
    <selectable>true</selectable>
    <tradeability>None</tradeability>
    <drawerType>RealtimeOnly</drawerType>
    <!-- For CompForbiddable -->
    <statBases>
      <MaxHitPoints>50</MaxHitPoints>
      <DeteriorationRate>2.0</DeteriorationRate>
      <Beauty>-8</Beauty>
      <Mass>1</Mass> <!-- ingredients are added to this value -->
    </statBases>
    <comps>
      <li Class="CompProperties_Forbiddable"/>
    </comps>
    <alwaysHaulable>true</alwaysHaulable>
    <rotatable>false</rotatable>
    <pathCost>15</pathCost>
    <thingCategories>
      <li>Unfinished</li>
    </thingCategories>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
      <li>Stony</li>
    </stuffCategories>
  </ThingDef>

  <ThingDef ParentName="RS_UnfinishedBase">
    <defName>RS_UnfinishedMetal</defName>
    <label>unfinished metal bars</label>
    <description>[Rimscape - Core] Unfinished metal bars.</description>
    <graphicData>
      <texPath>Things/Item/Unfinished/UnfinishedSculpture</texPath>
      <graphicClass>Graphic_Single</graphicClass>
    </graphicData>
    <stuffCategories>
      <li>Metallic</li>
    </stuffCategories>
  </ThingDef>

</Defs>
